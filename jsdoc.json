{
    "source": {
        "include": ["./"],
        "includePattern": ".js$",
        "excludePattern": "(node_modules)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "readme": "./readme.md",
        "destination": "docs/",
        "recurse": true,
        "verbose": true,
        "tutorials": "./tutorials"
    }
}
